class FileUpoladsController < ApplicationController
  # GET /file_upolads
  # GET /file_upolads.xml
  def index
    @file_upolads = FileUpolad.all

    
   render :layout => false
  end

  # GET /file_upolads/1
  # GET /file_upolads/1.xml
  def show
    @file_upolad = FileUpolad.find(params[:id])
    render :layout => false
    return

    respond_to do |format|
      format.html # show.html.erb
      format.xml  { render :xml => @file_upolad }
    end
  end

  # GET /file_upolads/new
  # GET /file_upolads/new.xml
  def new
    @file_upolad = FileUpolad.new
     render :layout => false
     
  end

  # GET /file_upolads/1/edit
  def edit
    @file_upolad = FileUpolad.find(params[:id])
  end

  # POST /file_upolads
  # POST /file_upolads.xml
  def create
    @file_upolad = FileUpolad.create(params[:file_upolad])
    render :layout => false
    return

    respond_to do |format|
      if @file_upolad.save
        format.html { redirect_to(@file_upolad, :notice => 'File upolad was successfully created.') }
        format.xml  { render :xml => @file_upolad, :status => :created, :location => @file_upolad }
         render :layout => false
      else
        format.html { render :action => "new" }
        format.xml  { render :xml => @file_upolad.errors, :status => :unprocessable_entity }
         render :layout => false
      end
    end
  end

  # PUT /file_upolads/1
  # PUT /file_upolads/1.xml
  def update
    @file_upolad = FileUpolad.find(params[:id])
    @file_upolad.update_attributes(params[:file_upolad])
    render :layout => false
    return

    respond_to do |format|
      if @file_upolad.update_attributes(params[:file_upolad])
        format.html { redirect_to(@file_upolad, :notice => 'File upolad was successfully updated.') }
        format.xml  { head :ok }
      else
        format.html { render :action => "edit" }
        format.xml  { render :xml => @file_upolad.errors, :status => :unprocessable_entity }
      end
    end
  end

  # DELETE /file_upolads/1
  # DELETE /file_upolads/1.xml
  def destroy
    @file_upolad = FileUpolad.find(params[:id])
    @file_upolad.destroy

    respond_to do |format|
      format.html { redirect_to(file_upolads_url) }
      format.xml  { head :ok }
    end
  end
end
